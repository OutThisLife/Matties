RewriteEngine on

RewriteCond %{HTTP_HOST} ^(www.)?mattiesaustin.com$

RewriteCond %{REQUEST_URI} !^/public/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /public/$1
RewriteCond %{HTTP_HOST} ^(www.)?mattiesaustin.com$
RewriteRule ^(/)?$ public/index.php [L]

<Files ~ ".env">
Deny from all
</Files>